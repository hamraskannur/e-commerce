
<%- include( '../includes/user_includes/head.ejs' ) %>
<%-include('../includes/user_includes/navegation.ejs')%>
<!-- Slider -->
<section class="section-slide">
	<div class="wrap-slick1">
		<div class="slick1">
			<% for( b of banner[0].items ) { %>
				<div class="item-slick1" style="background-image: url(/banner_image/<%=b.image%>);">
					<div class="container h-full">
						<div class="flex-col-l-m h-full p-t-100 p-b-30 respon5">
							<div class="layer-slick1 animated visible-false" data-appear="fadeInDown" data-delay="0">
								<span class="ltext-101 cl2 respon2">
									<%=b.title%>
								</span>
							</div>

							<div class="layer-slick1 animated visible-false" data-appear="fadeInUp" data-delay="800">
								<h2 class="ltext-201 cl2 p-t-19 p-b-43 respon1">
									<%=b.title2%>
								</h2>
							</div>

							<div class="layer-slick1 animated visible-false" data-appear="zoomIn" data-delay="1600">
								<a href="<%=b.productlink%>"
									class="flex-c-m stext-101 cl0 size-101 bg1 bor1 hov-btn1 p-lr-15 trans-04">
									Shop Now
								</a>
							</div>
						</div>
					</div>
				</div>
				
				<% } %>
		</div>
	</div>
</section>


<!-- Banner -->
<div class="sec-banner bg0 p-t-80 p-b-50">

	<div class="container">
		<div class="row">
			<% for( c of banner[1].items ) { %>

				<div class="col-md-6 col-xl-4 p-b-30 m-lr-auto">
					<div class="block1 wrap-pic-w">
						<img src="/banner_image/<%=c.image%>" alt="IMG-BANNER">

						<a onclick="getCategory('<%=c.category%>')"
							class="block1-txt ab-t-l s-full flex-col-l-sb p-lr-38 p-tb-34 trans-03 respon3">
							<div class="block1-txt-child1 flex-col-l">
								<span class="block1-name ltext-102 trans-04 p-b-8">
									<%=c.category%>
								</span>

								<span class="block1-info stext-102 trans-04">
									<%=c.title%>
								</span>
							</div>

							<div class="block1-txt-child2 p-b-4 trans-05">
								<div class="block1-link stext-101 cl0 trans-09">
									Shop Now
								</div>
							</div>
						</a>
					</div>
				</div>
				<% } %>

		</div>
	</div>
</div>


<!-- Product -->
<section class="bg0 p-t-23 p-b-140">
	<div class="container">
		<div class="p-b-10">
			<h3 class="ltext-103 cl5">
				Product Overview
			</h3>
		</div>

		<div class="flex-w flex-sb-m p-b-52">
			<div class="flex-w flex-l-m filter-tope-group m-tb-10">
				<a href="/" class=" stext-106 cl6 hov1 bor3 trans-04 m-r-32 m-tb-5 how-active1">
					All Products
				</a>
				<a onclick="getCategory('Electronics')"
					class="btn btn stext-106 cl6 hov1 bor3 trans-04 m-r-32 m-tb-5">
					Electronics
				</a>
				<a onclick="getCategory('Books')"
					class="btn btn stext-106 cl6 hov1 bor3 trans-04 m-r-32 m-tb-5">
					Books
				</a>
				<a onclick="getCategory('Phone')"
					class="btn btn stext-106 cl6 hov1 bor3 trans-04 m-r-32 m-tb-5">
					Phone
				</a>
				<a onclick="getCategory('Men dress')"
					class="btn btn stext-106 cl6 hov1 bor3 trans-04 m-r-32 m-tb-5">
					Men dress
				</a>
				<a onclick="getCategory('Kids dress')"
					class="btn btn stext-106 cl6 hov1 bor3 trans-04 m-r-32 m-tb-5">
					Kids dress
				</a>
				
			</div>

			<div class="flex-w flex-c-m m-tb-10">
				<div class="flex-c-m stext-106 cl6 size-104 bor4 pointer hov-btn3 trans-04 m-r-8 m-tb-4 js-show-filter">
					<i class="icon-filter cl2 m-r-6 fs-15 trans-04 zmdi zmdi-filter-list"></i>
					<i class="icon-close-filter cl2 m-r-6 fs-15 trans-04 zmdi zmdi-close dis-none"></i>
					Filter
				</div>

				<div class="flex-c-m stext-106 cl6 size-105 bor4 pointer hov-btn3 trans-04 m-tb-4 js-show-search">
					<i class="icon-search cl2 m-r-6 fs-15 trans-04 zmdi zmdi-search"></i>
					<i class="icon-close-search cl2 m-r-6 fs-15 trans-04 zmdi zmdi-close dis-none"></i>
					Search
				</div>
			</div>

			<!-- Search product -->
			<div class="dis-none panel-search w-full p-t-10 p-b-15">
				<div class="bor8 dis-flex p-l-15">
					<button class="size-113 flex-c-m fs-16 cl2 hov-cl1 trans-04">
						<i class="zmdi zmdi-search"></i>
					</button>

					<input class="mtext-107 cl2 size-114 plh2 p-r-15" id="mysearchInput"  onkeyup="mysearch()"
						type="text" name="search-product" placeholder="Search">
				</div>
			</div>

			<!-- Filter -->
			<div class="dis-none panel-filter w-full p-t-10">
				<div class="wrap-filter flex-w bg6 w-full p-lr-40 p-t-27 p-lr-15-sm">
					<div class="filter-col1 p-r-15 p-b-27">
						<div class="mtext-102 cl2 p-b-15">
							category
						</div>
						<ul>

							<% if( Categorysub.length> 0 ) { %>
								<% for( Categorysub of Categorysub ) { %>
									<li class="p-b-6">
										<a onclick="getCategory('<%=Categorysub.Category%>')"
											class="filter-link stext-106 trans-04">
											<%=Categorysub.Category%>
										</a>

										</a>
									</li>
									
									<% } %>
										<% }%>
						</ul>
					</div>

					<div class="filter-col2 p-r-15 p-b-27">
						<div class="mtext-102 cl2 p-b-15">
							Price
						</div>

						<ul>
							<li class="p-b-6">
								<a href="/" class="filter-link stext-106 trans-04 filter-link-active">
									All
								</a>
							</li>

							<li class="p-b-6">
								<a onclick="pricesort('0','500')" class="filter-link stext-106 trans-04">
									<i class="bi bi-currency-rupee"></i>0 - <i class="bi bi-currency-rupee"></i>500
								</a>
							</li>

							<li class="p-b-6">
								<a onclick="pricesort('500','1000')" class="filter-link stext-106 trans-04">
									<i class="bi bi-currency-rupee"></i>500 - <i class="bi bi-currency-rupee"></i>1000
								</a>
							</li>

							<li class="p-b-6">
								<a onclick="pricesort('1000','1500')" class="filter-link stext-106 trans-04">
									<i class="bi bi-currency-rupee"></i>1000 - <i class="bi bi-currency-rupee"></i>1500
								</a>
							</li>

							<li class="p-b-6">
								<a onclick="pricesort('1500','2000')" class="filter-link stext-106 trans-04">
									<i class="bi bi-currency-rupee"></i>1500 - <i class="bi bi-currency-rupee"></i>2000
								</a>
							</li>

							<li class="p-b-6">
								<a onclick="pricesort('2000','5000')" class="filter-link stext-106 trans-04">
									<i class="bi bi-currency-rupee"></i>2000 - <i class="bi bi-currency-rupee"></i>5000
								</a>
							</li>
						</ul>
					</div>



				</div>
			</div>
		</div>

		<div id="hi" class="row isotope-grid">
			<ul id="myUL">

				<% if( products ) { %>
					<% for( p of products ) { %>
						<li>
							<div class="col-sm-6 col-md-4 col-lg-3 p-b-35 isotope-item ">
								<!-- Block2 -->

								<div class="block2">
									<div class="block2-pic hov-img0">
										<img href="/viewsOneproduct/<%=p._id%>" src="/product-images/<%=p.Images[0]%>" alt="IMG-PRODUCT">

										<button onclick="Addtocart('<%=p._id%>')" class="block2-btn flex-c-m stext-103 cl2 size-102 bg0 bor2 hov-btn1 p-lr-15  trans-04 ">
											Add to cart
										</button>

									</div>

									<div class="block2-txt flex-w flex-t p-t-14">
										<div class="block2-txt-child1 flex-col-l ">
											<a href="/viewsOneproduct/<%=p._id%>"
												class="stext-104 cl4 hov-cl1 trans-04 js-name-b2 p-b-6">
												<%=p.title%>
											</a>

											<a href="/viewsOneproduct/<%=p._id%>" class="stext-105 cl3">
												<%=p.price%>
											</a>
										</div>

										<div class="block2-txt-child2 flex-r p-t-3">
											<% let flag=false %>
												<% if( wishlist) { %>
													<% for( i=0 ; i < wishlist.items.length ; i++ ) { %>
														<% if( wishlist.items[i].productId==p._id) { %>
															<% flag=true %>
																<% } %>
																	<% } %>
																		<% } %>
																			<% if( flag===true) { %>
											                              <img onclick="deleteWishlistProduct('<%=p._id%>','<%=p.title%>')" class="icon-heart dis-block " src="/user/images/icons/icon-heart-02.png"alt="ICON">
																		<% } else { %>
											                            <a onclick="addwishlist('<%=p._id%>','<%=p.title%>')" class="btn-addwish-b2 dis-block pos-relative ">
											                       <img id="addwishlistchange" class="icon-heart dis-block "src="/user/images/icons/icon-heart-01.png" alt="ICON">
																			<% } %>
																		</a>
										</div>
									</div>
								</div>
							</div>
						</li>

						<% } %>
			</ul>
			<% } else { %>
				<div>
					<h1>comeing soon product</h1>
				</div>
				<% }%>
		</div>

		<!-- Load more -->
	
	</div>
</section>


<!-- Back to top -->
<div class="btn-back-to-top" id="myBtn">
	<span class="symbol-btn-back-to-top">
		<i class="zmdi zmdi-chevron-up"></i>
	</span>
</div>

<!-- Modal1 -->



<%- include( '../includes/user_includes/footer.ejs' ) %>

	<%- include( '../includes/user_includes/end.ejs' ) %>