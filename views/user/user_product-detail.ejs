
<%- include( '../includes/user_includes/head.ejs' ) %>
<%-include('../includes/user_includes/navegation.ejs')%>
		



	
	<!-- Product Detail -->
	<section class="sec-product-detail bg0 p-t-65 p-b-60 mt-5">
		<div class="container">
			<div class="row">
				<div class="col-md-6 col-lg-7 p-b-30">
					<div class="p-l-25 p-r-30 p-lr-0-lg">
						<div class="wrap-slick3 flex-sb flex-w">
							<div class="wrap-slick3-dots"></div>
							<div class="wrap-slick3-arrows flex-sb-m flex-w"></div>

							<div class="slick3 gallery-lb">
								<div class="item-slick3" data-thumb="/product-images/<%=product.Images[0]%>">
									<div class="wrap-pic-w pos-relative">
										<img  src="/product-images/<%=product.Images[0]%>" alt="IMG-PRODUCT">

										<a class="flex-c-m size-108 how-pos1 bor0 fs-16 cl10 bg0 hov-btn3 trans-04" href="/product-images/<%=product.Images[0]%>">
											<i class="fa fa-expand"></i>
										</a>
									</div>
								</div>

								<div class="item-slick3" data-thumb="/product-images/<%=product.Images[1]%>">
									<div class="wrap-pic-w pos-relative">
										<img src="/product-images/<%=product.Images[1]%>" alt="IMG-PRODUCT">

										<a class="flex-c-m size-108 how-pos1 bor0 fs-16 cl10 bg0 hov-btn3 trans-04" href="/product-images/<%=product.Images[1]%>">
											<i class="fa fa-expand"></i>
										</a>
									</div>
								</div>

								<div class="item-slick3" data-thumb="/product-images/<%=product.Images[2]%>">
									<div class="wrap-pic-w pos-relative">
										<img src="/product-images/<%=product.Images[2]%>" alt="IMG-PRODUCT">

										<a class="flex-c-m size-108 how-pos1 bor0 fs-16 cl10 bg0 hov-btn3 trans-04" href="/product-images/<%=product.Images[2]%>">
											<i class="fa fa-expand"></i>
										</a>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>

				<div class="col-md-6 col-lg-5 p-b-30">
					<div class="p-r-50 p-t-5 p-lr-0-lg">
						<h4 class="mtext-105 cl2 js-name-detail p-b-14">
							Name:<%=product.title%>
						</h4>

						<span class="mtext-106 cl2">
							price:<%=product.price%>
						</span>

						<p class="stext-102 cl3 p-t-23">
							Description:<%=product.description%>
						</p>
						
						<!--  -->
						<div class="p-t-33">
							<div class="flex-w flex-r-m p-b-10">
								<div class="size-204 flex-w flex-m respon6-next">
									<div class="wrap-num-product flex-w m-r-20 m-tb-10">
										<div class="btn-num-product-down cl8 hov-btn3 trans-04 flex-c-m">
											<i class="fs-16 zmdi zmdi-minus"></i>
										</div>

										<input class="mtext-104 cl3 txt-center num-product" type="number" id="oneproductquantity" name="quantity" value="1">

										<div class="btn-num-product-up cl8 hov-btn3 trans-04 flex-c-m">
											<i class="fs-16 zmdi zmdi-plus"></i>
										</div>
									</div>
                                  <div class="d-flex">
									<% let flag=false %>

									<% if( wishlist ) { %>
										<% for( i=0 ; i < wishlist.items.length ; i++  ) { %>
											<% if( wishlist.items[i].productId==product._id) { %>
												<% flag=true %>
												<% } %>
											<% }%>
											<% }%>
									<button onclick="Addtocartoneprod('<%=product._id%>')" type="button"  class="flex-c-m stext-101 cl0 size-101 bg1 bor1 hov-btn1 p-lr-15 trans-04 ">
										Add to cart 
									</button>
									<% if( flag==true) { %>
										<button onclick="deleteWishlistProduct('<%=product._id%>','<%=product.title%>')" type="button"  class="flex-c-m stext-101 cl0 size-101 bg1 bor1 hov-btn1 p-lr-15 trans-04 ">
											ADDED IN Wishlist 
										   </button>
									<% } else { %>
										<button onclick="addwishlist('<%=product._id%>','<%=product.title%>')" type="button"  class="flex-c-m stext-101 cl0 size-101 bg1 bor1 hov-btn1 p-lr-15 trans-04 ">
											ADD TO Wishlist
										   </button>
										<% } %>


								</div>
								</div>
							</div>	
						</div>
					</div>
				</div>
			</div>
		</div>
	</section>

	<!-- Related Products -->
	<section class="sec-relate-product bg0 p-t-45 p-b-105">
		<div class="container">
			<div class="p-b-45">
				<h3 class="ltext-106 cl5 txt-center">
					Related Products
				</h3>
			</div>
			
			<!-- Slide2 -->
			<div class="wrap-slick2">
			
				<div class="slick2">
					<% if( categoryProduct ) { %>
		
						<% for( p of categoryProduct ) { %>

					<div class="item-slick2 p-l-15 p-r-15 p-t-15 p-b-15">
						<!-- Block2 -->
						<div class="block2">
							<div class="block2-pic hov-img0">
								<a href="/viewsOneproduct/<%=p._id%>"><img href="/viewsOneproduct/<%=p._id%>" style="height: 25rem; width: 20rem;" src="/product-images/<%=p.Images[0]%>" alt="IMG-PRODUCT"></a>

								<button onclick="Addtocart('<%=p._id%>')" class="block2-btn flex-c-m stext-103 cl2 size-102 bg0 bor2 hov-btn1 p-lr-15  ">
									Add to cart
								</button>
							</div>

							<div class="block2-txt flex-w flex-t p-t-14">
								<div class="block2-txt-child1 flex-col-l ">
									<a href="/viewsOneproduct/<%=p._id%>"
										class="stext-104 cl4 hov-cl1 trans-04 js-name-b2 p-b-6">
										<%=p.title%>
									</a>

									<a href="/viewsOneproduct/<%=p._id%>" class="stext-105 cl3">
										<%=p.price%>
									</a>
								</div>

							
							</div>
						</div>
						

					</div>
					<% }%>

					<% }%>

				

				

				

				

				

					

				
				</div>
				
			</div>
		
		</div>
	</section>
		


	<!-- Back to top -->
	<div class="btn-back-to-top" id="myBtn">
		<span class="symbol-btn-back-to-top">
			<i class="zmdi zmdi-chevron-up"></i>
		</span>
	</div>

