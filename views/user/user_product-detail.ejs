											<a class="flex-c-m size-108 how-pos1 bor0 fs-16 cl10 bg0 hov-btn3 trans-04" href="/product-images/<%=p.Images[0]%>">
												<i class="fa fa-expand"></i>
											</a>
										</div>
									</div>

									<div class="item-slick3" data-thumb="/product-images/<%=product.Images[2]%>">
										<div class="wrap-pic-w pos-relative">
											<img src="/product-images/<%=product.Images[2]%>" alt="IMG-PRODUCT">

											<a class="flex-c-m size-108 how-pos1 bor0 fs-16 cl10 bg0 hov-btn3 trans-04" href="/product-images/<%=product.Images[2]%>">
												<i class="fa fa-expand"></i>
											</a>
										</div>
