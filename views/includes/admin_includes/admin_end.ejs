  function changestatus(Status, orderID, insideOrderId) {
    const swalWithBootstrapButtons = Swal.mixin({
      customClass: {
        confirmButton: 'btn btn-success',
        cancelButton: 'btn btn-danger'
      },
      buttonsStyling: false
    })

    swalWithBootstrapButtons.fire({
      title: 'Are you sure?',
      text: "change Oder status",
      icon: 'warning',
      showCancelButton: true,
      confirmButtonText: 'Yes',
      cancelButtonText: 'No',
      reverseButtons: true
    }).then((result) => {
      if (result.isConfirmed) {
        console.log("hi");
        $.ajax({
          url: '/admin/changeStatus/' + Status + "/" + orderID + "/" + insideOrderId,
          method: 'get',
          success: (response) => {
            console.log(response);
            if (response) {
              swalWithBootstrapButtons.fire(
                'Status changed !',
                'changed status.' + Status,
                'success'
              ).then(() => {
                location.reload()

              })
            } else {
              swalWithBootstrapButtons.fire(
                'Sorry',
                'network proble try again',
                'error'
              )
            }


          }
        })
      } else if (result.dismiss === Swal.DismissReason.cancel) {
        swalWithBootstrapButtons.fire(
          'Cancelled',
          'Your imaginary file is safe :)',
          'error'
        )
      }
    })


  }

  function adminlogin() {
    let loginPassword = document.getElementById("loginPassword").value
    let loginEmail = document.getElementById("loginEmail").value

    $.ajax({
      url: '/admin/admin_login',
      data: {
        loginPassword,
        loginEmail
      },
      method: 'post',
      success: (response) => {
        if (response.Status) {
          location.href = '/admin'
        } else {
          $("#loginPassword").after(`<span class="error text-danger"> ${response.loginMessage} </span>`);

        }

      }

    })
  }

