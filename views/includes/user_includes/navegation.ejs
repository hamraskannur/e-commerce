


<body class="animsition">

	
	<!-- Header -->
	<header>
		<!-- Header desktop -->
		<div class="container-menu-desktop">
			<!-- Topbar -->
			<div class="top-bar">
				<div class="content-topbar flex-sb-m h-full container">
					<div class="left-top-bar">
						 shipping for free
					</div>

					<div class="right-top-bar flex-w h-full">
						
						<% if(login=="true"){%>
						<a href="/getMyAccount" class="flex-c-m trans-04 p-lr-25">
							My Account
						</a>
						
						<% }%>
						<% if(login=="true"){%>
						<a href="/logout" class="flex-c-m trans-04 p-lr-25">
							logout
						</a>
						<%}else{%>

						<a href="/login" class="flex-c-m trans-04 p-lr-25">
							log in
						</a>
						<a href="/signup" class="flex-c-m trans-04 p-lr-25">
							signup
						</a>
						<% }%>

					</div>
				</div>
			</div>

			<div class="wrap-menu-desktop">
				<nav class="limiter-menu-desktop container">
					
					<!-- Logo desktop -->		
					<a  class="logo">
						<img src="https://1000logos.net/wp-content/uploads/2017/02/Symbol-of-the-iPhone-logo.jpg" alt="IMG-LOGO">
					</a>

					<!-- Menu desktop -->
					<div class="menu-desktop">
						<ul class="main-menu">
							<li class="active-menu">
								<a href="/userhome">Home</a>
							</li>

							<li>
								<a href="/product">Products</a>
							</li>

							<li class="label1" data-label1="hot">
								<a href="/orderslist">orders</a>
							</li>

							<li>
								<a href="/Contact">Contact</a>
							</li>
						</ul>
					</div>	
				
					<!-- Icon header -->
					<div class="wrap-icon-header flex-w flex-r-m">
						<% if(login=="true"){%>
							<a href="/logout" style="color: white;" class="btn btn-secondary ps-4 ms-5">logout</a>
						<%}else{%>
							<div class="dropdown">
								<button class="btn btn-dark dropdown-toggle" type="button" id="dropdownMenuButton1" data-bs-toggle="dropdown" aria-expanded="false">
										My Account
								</button>
								<ul class="dropdown-menu bg-dark" aria-labelledby="dropdownMenuButton1">
								  <li><a href="/login" style="color: white;" class="text-white bg-dark ps-4 ms-2">login</a></li>
								  <hr style="color: white;">
								  <li><a href="/signup" style="color: white;" class="text-white bg-dark ps-4 ms-2">signup</a></li>
								</ul>
							  </div>
							
						<% }%>
						<% if(cartcount>0){%>
						<button onclick="popupcart()" class="icon-header-item cl2 hov-cl1 trans-04 p-l-22 p-r-11 js-show-cart" >
							<i  class="zmdi zmdi-shopping-cart "></i><span style="width: 1rem; height: 1rem; font-size: 10px;" class="badge badge-primary" id="cartcount"><%=cartcount%></span>
						</button>
						
						<% }%>


						<a href="/getwishlistpage"  class="dis-block icon-header-item cl2 hov-cl1 trans-04 p-l-22 p-r-11 " >
							<i class="zmdi zmdi-favorite-outline"></i><span style="width: 1rem; height: 1rem; font-size: 10px;" class="badge badge-primary" id="wishlistcount"><%=wishlistcount%></span>
						</a>
					</div>
					
				</nav>
			</div>	
		</div>

		<!-- Header Mobile -->
		<div class="wrap-header-mobile">
			<!-- Logo moblie -->		
			<div class="logo-mobile">
				<a ><img src="https://1000logos.net/wp-content/uploads/2017/02/Symbol-of-the-iPhone-logo.jpg" alt="IMG-LOGO"></a>
			</div>

			<!-- Icon header -->
			<div class="wrap-icon-header flex-w flex-r-m m-r-15">
				<% if(login=="true"){%>
					<a href="/logout" style="color: white;" class="btn btn-secondary ps-4 ms-5">logout</a>
				<%}else {%>
					<div class="dropdown">
						<button class="btn btn-dark dropdown-toggle" type="button" id="dropdownMenuButton1" data-bs-toggle="dropdown" aria-expanded="false">
								My Account
						</button>
						<ul class="dropdown-menu bg-dark" aria-labelledby="dropdownMenuButton1">
						  <li><a href="/login" style="color: white;" class="text-white bg-dark ps-4 ms-2">login</a></li>
						  <hr style="color: white;">
						  <li><a href="/signup" style="color: white;" class="text-white bg-dark ps-4 ms-2">signup</a></li>
						</ul>
					  </div>
				<% }%>
				<div class="icon-header-item cl2 hov-cl1 trans-04 p-r-11 js-show-modal-search">
					<i class="zmdi zmdi-search"></i>
				</div>
				<% if(cartcount > 0){%>
					
					<a onclick="popupcart()"  class="icon-header-item cl2 hov-cl1 trans-04 p-l-22 p-r-11 js-show-cart" >
						<i  class="zmdi zmdi-shopping-cart "></i><span style="width: 1rem; height: 1rem; font-size: 10px;" class="badge badge-primary" id="cartcount"><%=cartcount%></span>
					</a>
					<% }%>


						<a href="/getwishlistpage"  class="dis-block icon-header-item cl2 hov-cl1 trans-04 p-l-22 p-r-11 " >
							<i class="zmdi zmdi-favorite-outline"></i><span style="width: 1rem; height: 1rem; font-size: 10px;" class="badge badge-primary" id="wishlistcount"><%=wishlistcount%></span>
						</a>
			</div>

			<!-- Button show menu -->
			<div class="btn-show-menu-mobile hamburger hamburger--squeeze">
				<span class="hamburger-box">
					<span class="hamburger-inner"></span>
				</span>
			</div>
		</div>


		<!-- Menu Mobile -->
		<div class="menu-mobile">
			<ul class="topbar-mobile">
				<li>
					<div class="left-top-bar">
						shipping for free
					</div>
				</li>

				<li>
					<div class="right-top-bar flex-w h-full">
						<% if(login=="true"){%>
						<a href="/getMyAccount" class="flex-c-m p-lr-10 trans-04">
							My Account
						</a>
						<% }%>
						<% if(login=="true"){%>
						<a href="/logout" class="flex-c-m p-lr-10 trans-04">
							logout
						</a>
						<%}else{%>
							<a href="/login" class="flex-c-m p-lr-10 trans-04">
								log in
							</a>
						<a href="/signup" class="flex-c-m p-lr-10 trans-04">
							signup
						</a>
						<% }%>

					</div>
				</li>
			</ul>
			<ul class="main-menu-m">
				<li>
					<a href="/userhome">Home</a>
				</li>

				<li>
					<a href="/product">products</a>
				</li>

				<li>
					<a  href="/orderslist">orders</a>
				</li>


				<li>
					<a href="/Contact">Contact</a>
				</li>
			</ul>
		</div>

		<!-- Modal Search -->
		<div class="modal-search-header flex-c-m trans-04 js-hide-modal-search">
			<div class="container-search-header">
				<button class="flex-c-m btn-hide-modal-search trans-04 js-hide-modal-search">
					<img src="/user/images/icons/icon-close2.png" alt="CLOSE">
				</button>

				<form class="wrap-search-header flex-w p-l-15">
					<button class="flex-c-m trans-04">
						<i class="zmdi zmdi-search"></i>
					</button>
					<input class="plh3" type="text" name="search" placeholder="Search...">
				</form>
			</div>
		</div>
<!-- ///////////////////////////////////////////// -->




	

<div  class="wrap-header-cart js-panel-cart">
	<div class="s-full js-hide-cart"></div>

	<div class="header-cart flex-col-l p-l-65 p-r-25">
		<div class="header-cart-title flex-w flex-sb-m p-b-8">
			<span class="mtext-103 cl2">
				Your Cart
			</span>

			<div class="fs-35 lh-10 cl2 p-lr-5 pointer hov-cl1 trans-04 js-hide-cart">
				<i class="zmdi zmdi-close"></i>
			</div>
		</div>
		
		<div class="header-cart-content flex-w js-pscroll">
			<ul class="header-cart-wrapitem w-full">
				<div id="userPopupCart">
				<% if( usercart ) { %>
				<% for(element of usercart.items) { %>
					
				<li  class="header-cart-item flex-w flex-t m-b-12">
					<div class="header-cart-item-img">
						<img src="/product-images/<%=element.images[0]%>" alt="IMG">
					</div>

					<div class="header-cart-item-txt p-t-8">
						<a href="#" class="header-cart-item-name m-b-18 hov-cl1 trans-04">
							<%=element.name%>
						</a>

						<span class="header-cart-item-info">
							<%=element.quantity%> x <%=element.price%>
						</span>
					</div>
				</li>
				<% } %>
				<% }%>
			</div>

			
			</ul>
			
			<div class="w-full">
				<% if( usercart ) { %>

				<div id="TotalPricePopupCart" class="header-cart-total w-full p-tb-40">
					Total: <i class="bi bi-currency-rupee"></i><%=usercart.bill%>
				</div>
				<% } %>

				<div class="header-cart-buttons flex-w w-full">
					<a href="/viewcartmycart" class="flex-c-m stext-101 cl0 size-107 bg3 bor2 hov-btn3 p-lr-15 trans-04 m-r-8 m-b-10">
						View Cart
					</a>

				
				</div>
			</div>
		</div>
	</div>
</div>
<!-- /////////////////// -->
	</header>
