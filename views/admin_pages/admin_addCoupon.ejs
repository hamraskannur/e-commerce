    <!-- Carousel Start -->
    <div class="container-fluid mb-3">
        <div class="row px-xl-5">

              <div class="col-lg-9 col-md-9">
                <h4 class="section-title position-relative text-uppercase mb-3">
                  <span class="bg-secondary pr-3">Add New Coupon</span>
                </h4>
                <div>
                        <div class="alert alert-danger" role="alert">
                        </div>
                    <div 
                        id="login-row" 
                        class="row justify-content-center align-items-center"
                        >
                        <div 
                            id="login-column" 
                            class="col-md-6">
                                <div 
                                    id="login-box" 
                                    class="col-md-12">
                                        <form  id="login-form" 
                                            class="form" 
                                            action="/admin/admin-addCoupon" 
                                            method="post">
                                            <div class="form-group">
                                                <label 
                                                    for="email" 
                                                    class="text-dark">
                                                        New Coupon name:
                                                </label>
                                                <br>
                                                <input 
                                                    type="text" 
                                                    name="CouponName" 
                                                    id="email" 
                                                    class="form-control border"
                                                    required
                                                >
                                            </div>
                                            <div class="form-group">
                                                <label 
                                                    for="email" 
                                                    class="text-dark">
                                                        Discount Percentage:
                                                </label>
                                                <br>
                                                <input 
                                                    type="number" 
                                                    name="discount" 
                                                    id="percentage" 
                                                    class="form-control border"
                                                
                                                    required
                                                >
                                            </div>
                                            <div class="form-group">
                                                <label 
                                                    for="email" 
                                                    class="text-dark">
                                                        Maximom Discount:
                                                </label>
                                                <br>
                                                <input 
                                                    type="number" 
                                                    name="maxlimite" 
                                                    id="email" 
                                                    class="form-control border"
                                                    required
                                                >
                                            </div>
                                            <div class="form-group">
                                                <label 
                                                    for="email" 
                                                    class="text-dark">
                                                        Minimum Amount:
                                                </label>
                                                <br>
                                                <input 
                                                    type="number" 
                                                    name="minpurchase" 
                                                    id="email" 
                                                    class="form-control border"
                                                    required
                                                >
                                            </div>
                                          
                                            <div class="form-group">
                                                <label 
                                                    for="email" 
                                                    class="text-dark">
                                                    expdate :
                                                </label>
                                                <br>
                                                <input 
                                                    type="date" 
                                                    name="expdate" 
                                                    id="email" 
                                                    class="form-control border"
                                                    required
                                                >
                                            </div>
                                            <div class="align-items-center justify-content-center">

                                              <small class="text-danger" ><%=addCouponerr%></small>
                                            </div>
                                          
                                            <div class="form-group">
                                                <input 
                                                    type="submit" 
                                                    name="submit" 
                                                    class="btn btn-warning btn-md" 
                                                    value="submit"
                                                >
                                            </div>
                                        </form>
